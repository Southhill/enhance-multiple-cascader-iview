{
  "name": "enhance-multiple-cascader",
  "version": "1.2.4",
  "description": "enhance multiple cascader base on iview",
  "main": "dist/enhance-multiple-cascader.js",
  "scripts": {
    "dev": "webpack-dev-server --open-page dist/index.html --inline --config build/webpack.dev.config.js",
    "build": "webpack --config build/webpack.prod.config.js",
    "lint": "standard --verbose | snazzy",
    "check": "bash ./scripts/check-version.sh"
  },
  "author": "Cao",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Southhill/enhance-multiple-cascader-iview.git"
  },
  "keywords": [
    "vue",
    "iview",
    "multiple-cascader",
    "enhance-form-component"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && standard --fix && git add .",
      "pre-push": "npm run check"
    }
  },
  "bugs": {
    "url": "https://github.com/Southhill/enhance-multiple-cascader-iview/issues"
  },
  "homepage": "https://github.com/Southhill/enhance-multiple-cascader-iview#readme",
  "dependencies": {
    "iview": "^3.5.3",
    "vue": "^2.5.9"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.4.2",
    "axios": "^0.18.1",
    "babel-loader": "^8.0.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^3.1.0",
    "jasmine-core": "^3.3.0",
    "karma": "^4.0.1",
    "karma-babel-preprocessor": "^8.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^2.0.1",
    "karma-webpack": "^3.0.5",
    "less": "^3.10.3",
    "less-loader": "^4.0.5",
    "snazzy": "^8.0.0",
    "standard": "^14.3.1",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.5.0",
    "vue-template-compiler": "^2.5.9",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^3.1.11",
    "webpack-merge": "^4.1.1"
  },
  "standard": {
    "ignore": [
      "/dist/",
      "/build/"
    ]
  }
}
